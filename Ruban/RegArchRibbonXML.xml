<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab idMso="TabFormulas">
        <group id="RegArchGroup"
               label="RegArch">
          <menu id="modelMenu" label="Model">
            <button id="setBouton" label="Set" onAction="setBoutonClick"
                    enabled="true" supertip="Set model"/>
            <button id="loadBouton" label="Load" onAction="loadBoutonClick" 
                    enabled="true" supertip="Load model"/>
            <button id="saveBouton" label ="Save" onAction ="saveBoutonClick" 
                    supertip="Save model" getEnabled="enableDisableButton"/>
          </menu>
          <button id="simulationBouton" label="Simulation" onAction="simulationBoutonClick" 
                  supertip="Simulate model" getEnabled="enableDisableButton"/>
          <button id="estimationBouton" label="Estimation" onAction="estimateBoutonClick" 
                  supertip="Estimate model" getEnabled="enableDisableButton"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

